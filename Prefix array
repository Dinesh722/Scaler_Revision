public class Solution {
    public void solve(ArrayList<Integer> A, int B) {
        ArrayList<Integer> pf = new ArrayList<Integer>();
        ArrayList<Integer> sf = new ArrayList<Integer>();
        int n = A.size();
        for(int i=0;i<n;i++){
            if(i==0){
                pf.add(A.get(i));
            }else{
                pf.add(pf.get(i-1)+A.get(i));
            }
        }
    }
}
